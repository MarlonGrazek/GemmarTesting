{
  "name": "gemmartesting",
  "version": "0.0.7",
  "description": "Eine Desktop-Anwendung zum Testen von Code-Ãœbungen.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dist": "electron-builder"
  },
  "keywords": [
    "electron",
    "java",
    "test",
    "education"
  ],
  "author": "Marlon Grazek",
  "license": "ISC",
  "devDependencies": {
    "electron": "^36.3.1",
    "electron-builder": "^26.0.12"
  },
  "build": {
    "appId": "gemmartesting",
    "productName": "Gemmar Testing",
    "artifactName": "Gemmar-Testing-Setup-${version}.${ext}",
    "publish": {
      "provider": "github",
      "owner": "MarlonGrazek",
      "repo": "GemmarTesting"
    },
    "win": {
      "target": "nsis",
      "icon": "assets/icon.png"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    }
  },
  "dependencies": {
    "electron-log": "^5.4.0",
    "electron-updater": "^6.6.2"
  }
}
